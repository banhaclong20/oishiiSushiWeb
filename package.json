    "@types/react": "^18.0.25",
    "commitizen": "^4.2.5",
    "commitlint": "^17.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-sznm": "^1.0.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "next-sitemap": "^3.1.32",
