{
  "name": "oishii-restaurants",
  "version": "0.1.0",
  "private": true,
  "author": "totran",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
