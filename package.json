    "@formiz/validations": "^1.0.0",
    "@prisma/client": "^4.8.0",
    "@reduxjs/toolkit": "1.9.1",
    "@types/react-slick": "0.23.10",
    "bcrypt": "^5.1.0",
    "framer-motion": "^8.1.7",
    "next": "13.0.0",
    "next-auth": "4.17.0",
    "next-pwa": "^5.6.0",
    "next-redux-wrapper": "8.1.0",
    "next-seo": "5.15.0",
